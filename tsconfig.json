{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/api/profile/route.js",
    "app/profile/page.jsx",
    "app/[profile]/page.tsx",
    "app/story/[slug]/page.tsx",
    "app/explore/page.tsx",
    "app/create/page.jsx",
    "app/tag/[slug]/page.tsx",
    "app/tag/[slug]/taag.jsx",
    "app/sitemap.js",
    "app/api/user/route.js",
    "app/create/Editor.jsx",
    "app/story/[slug]/storys.jsx",
    "app/api/GetArticle/[slug]/route.js",
    "app/shot/[slug]/comment.tsx"
  ],
  "exclude": ["node_modules"]
}
